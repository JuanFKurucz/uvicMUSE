<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="Toplevel_1">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Frame" id="Frame_list">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">5</property>
          <property name="padx">20</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
          <rows>
            <row id="0">
              <property name="weight">0</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="minsize">600</property>
            </column>
          </columns>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Frame_10">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Button" id="Refresh">
            <property name="command">refresh_btn</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Refresh</property>
            <property name="textvariable">string:ref_var</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">n</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="Connect">
            <property name="command">connect_callback</property>
            <property name="text" translatable="yes">Connect</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="address_label">
            <property name="text" translatable="yes">UDP Address:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="address_entry">
            <property name="font">TkDefaultFont</property>
            <property name="justify">center</property>
            <property name="state">normal</property>
            <property name="text" translatable="yes">localhost</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="portt_label">
            <property name="text" translatable="yes">UDP Port</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="port_entry">
            <property name="font">TkDefaultFont</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">8888</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="Frame_1">
            <property name="height">60</property>
            <property name="width">150</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="UDP_send_btn">
            <property name="command">UDP_send_btn</property>
            <property name="compound">top</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Start UDP Stream</property>
            <property name="underline">0</property>
            <property name="width">20</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">5</property>
              <property name="pady">5</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="UDP_stop_btn">
            <property name="command">UDP_stop_btn</property>
            <property name="text" translatable="yes">Stop UDP Stream </property>
            <property name="width">20</property>
            <layout>
              <property name="column">0</property>
              <property name="ipady">7</property>
              <property name="propagate">False</property>
              <property name="row">8</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="Disconnect">
            <property name="command">disconnect_callback</property>
            <property name="compound">top</property>
            <property name="state">normal</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Disconnect</property>
            <layout>
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">n</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="text" translatable="yes">Log:</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">20</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Frame_11">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="img_label">
            <property name="compound">top</property>
            <property name="cursor">arrow</property>
            <property name="font">TkDefaultFont</property>
            <property name="width">0</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
